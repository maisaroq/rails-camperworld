<div class="container text-center">
  <%= link_to icon('fas fa-edit', "Edit"), edit_user_path(current_user), class: "btn pt-0 pr-0 text-muted" %>
  <h1> Your Profile </h1>
  <% if @user.avatar.attached? %>
    <%= cl_image_tag @user.avatar.key, class: "avatar-large" %>
  <% else %>
  <img src="data:image/png;base64," alt="user-avatar" class="avatar-large">
    <%= image_tag(@listing.listing_photo, class: "img-responsive", width: "100%") %>
  <% end %>
  <p> <strong>Name:</strong>  <%= @user.first_name %> </p>
  <p> <strong>Email:</strong>  <%= @user.email %> </p>
  <p> <strong>Phone number:</strong> (186)285-7925 </p>
  <p> <strong>Location:</strong> Berlin </p>
</div>
